{
  "name": "t2-avatar-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "deploy": "aws s3 --endpoint-url=https://storage.yandexcloud.net --profile yc-t2 sync ./dist s3://t2avatar.ru --size-only --delete",
    "bd": "yarn build && yarn deploy",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "konva": "^9.3.14",
    "vue": "^3.4.31",
    "vue-i18n": "9"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@types/node": "^20.14.12",
    "@unhead/vue": "^1.9.16",
    "@vitejs/plugin-vue": "^5.0.5",
    "sass": "^1.77.8",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vue-tsc": "^2.0.24"
  }
}
